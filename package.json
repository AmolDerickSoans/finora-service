{
  "name": "finora-service",
  "version": "0.0.1",
  "description": "Backend system for finora ,handles supa base auth , supabase postgres , gemini llm ,redis and other systems involved. Using hexagonal architecture pattern with distinct API, Service, Domain, and Infrastructure layers.",
  "main": "src/app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --respawn --transpile-only ./app.ts",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.{ts,js,json}'",
    "build:watch": "tsc --watch",
    "perf": "autocannon -c 10 -d 10 http://localhost:3000"
  },
  "author": "Amol Derick Soans",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0"
  },
  "devDependencies": {
    "@langchain/core": "^0.3.23",
    "@langchain/google-genai": "^0.1.5",
    "@types/autocannon": "^7.12.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/retry": "^0.12.5",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "autocannon": "^8.0.0",
    "dotenv-safe": "^9.1.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "swagger": "^0.7.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "winston": "^3.17.0"
  }
