{
  "compilerOptions": {  
    "target": "es2016",                                  
    "module": "commonjs",                              
    "sourceMap": false,                              
     "outDir": "./dist",                               
    "esModuleInterop": true,                          
    "forceConsistentCasingInFileNames": true,           
    "strict": true,                                   
    "skipLibCheck": true ,  
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "**/*.spec.ts"],
}
